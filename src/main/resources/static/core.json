{
  "entry": {
    "jvm": "-Xms1G -Xmx1G",
    "environment": {}
  },
  "common": {
    "column": {
      "datetimeFormat": "yyyy-MM-dd HH:mm:ss",
      "timeFormat": "HH:mm:ss",
      "dateFormat": "yyyy-MM-dd",
      "extraFormats": [
        "yyyyMMdd"
      ],
      "timeZone": "GMT+8",
      "encoding": "utf-8"
    }
  },
  "core": {
    "transport": {
      "channel": {
        "class": "com.alibaba.datax.core.transport.channel.memory.MemoryChannel"
      }
    },
    "container": {
      "taskGroup": {
        "channel": 5
      },
      "job": {
        "id": 1
      }
    }
  }
}
